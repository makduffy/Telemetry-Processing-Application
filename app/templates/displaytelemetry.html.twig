            </ul>
            <br>
            <br>
            <ul>
                {% for item in heaterdata %}
                    <p>{{ item }}</p>
                {% else %}
                    <p>No telemetry data found.</p>
                {% endfor %}
            </ul>
            <br>
            <br>
            <ul>
                {% for item in fandata %}
                    <p>{{ item }}</p>
                {% else %}
                    <p>No telemetry data found.</p>
                {% endfor %}
            </ul>
        </fieldset>
    </div>
{% endblock %}